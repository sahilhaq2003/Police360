{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build", "config": { "distDir": "dist" } }
  ],
  "routes": [
    { "src": "^/favicon.ico$", "dest": "/images/PLogo.png" },
    { "src": "^/api/(.*)$", "status": 404, "dest": "/404.html" },
    { "src": "^/(.*)$", "dest": "/index.html" }
  ]
}
